      spacing="20"
      alignment="CENTER"
      fx:controller="fx.components.orchestrator.OrchestratorController">

    <fx:include source="../header/Header.fxml" fx:id="header"/>

    <Separator style="-fx-background-color: black" orientation="horizontal"/>

    <StackPane>
