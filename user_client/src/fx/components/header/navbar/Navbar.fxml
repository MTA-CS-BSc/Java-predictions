<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:controller="fx.components.header.navbar.NavbarController"
      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="70.0">
    <Button fx:id="detailsButton" onAction="#handleDetailsClicked" mnemonicParsing="false" text="Details"/>
    <Button fx:id="requestsButton" onAction="#handleRequestsClicked" mnemonicParsing="false" text="Requests"/>
    <Button fx:id="executionButton" onAction="#handleExecutionClicked" mnemonicParsing="false" text="Execution"/>
    <Button fx:id="resultsButton" onAction="#handleResultsClicked" mnemonicParsing="false" text="Results"/>
</HBox>