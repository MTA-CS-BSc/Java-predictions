<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="container" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.views.NewExecution.NewExecutionController">
        <padding>
            <Insets bottom="20.0" top="20.0" />
        </padding>
        <HBox alignment="CENTER" spacing="50.0">
            <fx:include fx:id="populationTable" source="../PopulationTable/PopulationTable.fxml" />
            <TableView fx:id="envPropsTable" editable="true">
                <columns>
                    <TableColumn fx:id="propertyNameColumn" editable="false" prefWidth="150.0" text="Property Name" />
                    <TableColumn fx:id="propertyTypeColumn" editable="false" text="Type" />
                    <TableColumn fx:id="propertyRangeColumn" editable="false" prefWidth="170.0" text="Range" />
                    <TableColumn fx:id="propertyValueColumn" prefWidth="150.0" text="Value" />
                </columns>
            </TableView>
        </HBox>
        <HBox alignment="BOTTOM_CENTER">
            <padding>
                <Insets left="20.0" right="20.0" />
            </padding>
            <Button alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#handleClear" styleClass="new-execution-screen-button" text="CLEAR" />
            <Region HBox.hgrow="ALWAYS" />
            <Button alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#handleRun" styleClass="new-execution-screen-button" text="RUN" />
        </HBox>
</VBox>
