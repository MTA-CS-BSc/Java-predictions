<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.controllers.NewExecutionController">
   <children>
      <HBox spacing="50.0">
         <children>
            <TableView fx:id="populationTable">
              <columns>
                <TableColumn fx:id="entityColumn" prefWidth="100.0" text="Entity Name" />
                <TableColumn fx:id="populationColumn" prefWidth="100.0" text="Population" />
              </columns>
            </TableView>
            <TableView>
              <columns>
                <TableColumn prefWidth="150.0" text="Property Name" />
                <TableColumn text="Type" />
                  <TableColumn prefWidth="75.0" text="Range" />
                  <TableColumn prefWidth="250.0" text="Value" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox>
         <children>
            <Button alignment="BOTTOM_RIGHT" mnemonicParsing="false" text="CLEAR" />
            <Region HBox.hgrow="ALWAYS" />
            <Button alignment="TOP_LEFT" mnemonicParsing="false" text="RUN" />
         </children>
      </HBox>
   </children>
</VBox>
