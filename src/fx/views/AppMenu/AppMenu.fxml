<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.controllers.AppMenuController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Themes">
          <items>
            <MenuItem mnemonicParsing="false" text="Default" />
            <MenuItem mnemonicParsing="false" text="Dark" />
            <MenuItem mnemonicParsing="false" text="Windows 7" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </children>
</VBox>
