<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<VBox fx:id="container" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="fx.views.FinishedStats.FinishedStatsController" spacing="10">

    <HBox>
        <ComboBox fx:id="filterByComboBox" promptText="Filter by">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Entities amounts"/>
                    <String fx:value="Property statistics"/>
                </FXCollections>
            </items>
            <value>
                <String fx:value="Entities amounts"/>
            </value>
        </ComboBox>
    </HBox>

    <StackPane prefHeight="250.0">
        <fx:include source="EntitiesAmountChart.fxml" fx:id="entitiesAmountChart"/>
        <fx:include source="PropertyStats.fxml" fx:id="propertyStats"/>
    </StackPane>
</VBox>
