<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="fx.views.StatsTable.StatsTableController">

    <ComboBox fx:id="filterBy" promptText="Filter by" />
    <StackPane prefHeight="250.0">
        <LineChart fx:id="entitiesAmountChart">
            <xAxis>
                <NumberAxis fx:id="ticksAxis" tickUnit="1" tickLabelGap="2" label="Ticks" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="amountAxis" label="Entities amount" />
            </yAxis>
        </LineChart>
    </StackPane>
</VBox>
